@{
    ViewBag.Title = "About us";
    Layout = "~/Areas/User/Views/Shared/_Layout.cshtml";
}
<div id="page-wrapper">
    <div class="about-section">
        <h1>Về chúng tôi</h1>
        <p>Nhóm sinh viên đến từ Trường Đại học Công nghệ thông tin.</p>
    </div>

    <h2 style="text-align:center; font-size:30px; margin-top:10px">Thành viên nhóm</h2>
    <div class="row">
        <div class="column">
            <div class="card">
                <img class="img-card" src="https://res.cloudinary.com/dsh8zkuek/image/upload/v1671720414/Tien_ybrjtb.jpg" alt="Jane"/>
                <div class="text-card">
                    <h2>Trần Ngọc Mỹ Tiên</h2>
                    <p class="title">Nhóm trưởng</p>
                    <p>19522343@gm.uit.edu.vn</p>
                    <p>Khơi nguồn sáng tạo</p>

                </div>
            </div>
        </div>

        <div class="column">
            <div class="card">
                <img class="img-card" src="https://res.cloudinary.com/dsh8zkuek/image/upload/v1671720468/58586664_830731983949944_2332117205359001600_n_iu5agc.jpg" alt="Jane"  />
                <div class="text-card">
                    <h2>Nguyễn Thị Mai Trinh</h2>
                    <p class="title">Thành viên</p>
                    <p>19522405@gm.uit.edu.vn</p>
                    <p>Kết nối giá trị cuộc sống</p>

                </div>
            </div>
        </div>

        <div class="column">
            <div class="card">
                <img class="img-card"  src="https://res.cloudinary.com/dsh8zkuek/image/upload/v1671722591/Phuc_olnjjg.jpg" alt="Jane" />
                <div class="text-card">
                    <h2>Lưu Phúc</h2>
                    <p class="title">Thành viên</p>
                    <p>19522034@gm.uit.edu.vn</p>
                    <p>Luôn mang đến những nguồn vui</p>

                </div>
            </div>
        </div>
    </div>
</div>

